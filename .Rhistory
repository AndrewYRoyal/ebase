Start <- Sys.time()
# Specify min number of days
ebDT <- ebDataFormat(useDT = useDT,
meterDT = meterDT,
base.length = 24,
date.format = '%m/%d/%Y %H',
padding = 1,
interval = 'hourly')
ebModelList <- ebModel(ebDT, type = 'GBoost')
